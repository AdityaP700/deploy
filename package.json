{
  "name": "metacall-upload",
  "version": "0.1.2",
  "description": "Tool for deploying into MetaCall FaaS platform.",
  "main": "dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metacall/upload.git"
  },
  "keywords": [
    "MetaCall",
    "FaaS",
    "upload",
    "tool"
  ],
  "author": "Thomas Rory Gummerson <thomas@gummerson.no> (https://trgwii.no/)",
  "contributors": [
    "Vicente Eduardo Ferrer Garcia <vic798@gmail.com> (https://metacall.io/)"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/metacall/upload/issues"
  },
  "homepage": "https://github.com/metacall/upload#readme",
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "eslint:recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "@typescript-eslint/indent": [
        "error",
        "tab"
      ],
      "@typescript-eslint/explicit-function-return-type": "off"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "ini": "^1.3.5",
    "inquirer": "^6.3.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/ini": "^1.3.30",
    "@types/inquirer": "^6.0.3",
    "@types/node": "^12.0.8",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "typescript": "^3.5.2"
  }
}
